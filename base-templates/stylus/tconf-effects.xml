<messages active="false">
    <message id="boxShadow" height="300px">
        <tailwind-docs>https://tailwindcss.com/docs/box-shadow</tailwind-docs>
        <reset-command>shadow-res</reset-command>
        <classes className="shadow" customValue="true" canExtend="true">
            <class>
                <name>sm</name>
                <value>0 1px 2px 0 rgb(0 0 0 / 0.05)</value>
            </class>
            <class>
                <name>DEFAULT</name>
                <value>["0 1px 3px 0 rgb(0 0 0 / 0.1)", "0 1px 2px -1px rgb(0 0 0 / 0.1)"]</value>
            </class>
            <class>
                <name>md</name>
                <value>["0 4px 6px -1px rgb(0 0 0 / 0.1)", "0 2px 4px -2px rgb(0 0 0 / 0.1)"]</value>
            </class>
            <class>
                <name>lg</name>
                <value>["0 10px 15px -3px rgb(0 0 0 / 0.1)", "0 4px 6px -4px rgb(0 0 0 / 0.1)"]</value>
            </class>
            <class>
                <name>xl</name>
                <value>["0 20px 25px -5px rgb(0 0 0 / 0.1)", "0 8px 10px -6px rgb(0 0 0 / 0.1)"]</value>
            </class>
            <class>
                <name>2xl</name>
                <value>0 25px 50px -12px rgb(0 0 0 / 0.25)</value>
            </class>
            <!-- <class><name>inner</name><value>inset 0 2px 4px 0 rgb(0 0 0 / 0.05)</value></class> -->
            <class>
                <name>none</name>
                <value>none</value>
            </class>
            <class extends="true">
                <name>test</name>
                <value>0 0 10px #0000</value>
            </class>
        </classes>
    </message>
    <message id="boxShadowInner" height="300px">
        <tailwind-docs>https://tailwindcss.com/docs/box-shadow</tailwind-docs>
        <reset-command>shadow-inner-res</reset-command>
        <classes className="shadow" customValue="false" canExtend="false">
            <class>
                <name>inner</name>
                <value>inset 0 2px 4px 0 rgb(0 0 0 / 0.05)</value>
            </class>
        </classes>
    </message>
